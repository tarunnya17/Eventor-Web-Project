<div class="relative bg-white rounded-lg shadow dark:bg-gray-700 p-6">
  <!-- Modal header -->
  <div class="flex items-start justify-between border-b rounded-t dark:border-gray-600">
    <h3 class="text-xl font-semibold text-gray-900 dark:text-white">
      Static modal
    </h3>
    <button type="button" name="closeTicket" class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ml-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white" data-modal-hide="ticketInfo<%=index%>">
      <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 14">
        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
      </svg>
      <span class="sr-only">Close modal</span>
    </button>
  </div>
  <!-- Modal body -->
  <div>
    <% var row = Object.keys(types.tickets).length %>

    <% for( let j = 0; j < row; j++ ) { %>
    <div class="flex">
      <%var col = Object.keys(types.tickets[j]).length %>
      <% for( let k = 0; k < col; k++ ) { %>
      <% var bg = 'bg-red-500' %>
      <% var act = 'disable' %>
      <% if (types.tickets[j][k].Available) { %>
      <%  bg = 'bg-slate-200' %>
      <% act = '' %>
      <% } %>
      <div name="ticketSelectorPDiv-<%=index%>">
        <input type="hidden" name="row-<%=index%>" value="<%= j %>">
        <input type="hidden" name="col-<%=index%>" value="<%= k %>">
        <button name="ticketSelectorButton-<%=index%>" class="<%= bg %> p-2 m-2 rounded-full whitespace-nowrap  w-15 h-15 text-xs" <%= act %>>
          <%= types.tickets[j][k].sitId %>
        </button>
      </div>
      <% } %>
    </div>
    <% } %>
  </div>
</div>